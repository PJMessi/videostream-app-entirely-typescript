    "module-alias": "^2.2.2",
    "multer": "^1.4.2",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^2.0.0",
    "sqlite3": "^5.0.0",
